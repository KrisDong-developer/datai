    <sql id="selectDataiIntegrationRateLimitVo">
        select 
            dirl.id,
            dirl.api_type,
            dirl.window_start,
            dirl.window_end,
            dirl.request_count,
            dirl.limit_threshold,
            dirl.remaining_requests,
            dirl.reset_time,
            dirl.is_throttled,
            dirl.create_time,
            dirl.update_time,
