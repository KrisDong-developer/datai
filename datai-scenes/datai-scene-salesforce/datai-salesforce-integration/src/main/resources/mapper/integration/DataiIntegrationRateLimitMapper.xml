            <if test="tenantId != null  and tenantId != ''"> and dirl.tenant_id = #{tenantId}</if>
        </where>
    </select>
    
    <select id="selectDataiIntegrationRateLimitById" parameterType="Long" resultMap="DataiIntegrationRateLimitResult">
