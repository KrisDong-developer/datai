<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.datai.setting.mapper.DataiConfigSnapshotMapper">
    
    <resultMap type="DataiConfigSnapshot" id="DataiConfigSnapshotResult">
        <result property="snapshotId" column="snapshot_id" />
        <result property="versionId" column="version_id" />
        <result property="snapshotTime" column="snapshot_time" />
        <result property="snapshotContent" column="snapshot_content" />
        <result property="configCount" column="config_count" />
        <result property="createBy" column="create_by" />
        <result property="createTime" column="create_time" />
        <result property="remark" column="remark" />
        <result property="tenantId" column="tenant_id" />
    </resultMap>

    <sql id="selectDataiConfigSnapshotVo">
        select 
            dcs.snapshot_id,
            dcs.version_id,
            dcs.snapshot_time,
            dcs.snapshot_content,
            dcs.config_count,
            dcs.create_by,
            dcs.create_time,
            dcs.remark,
            dcs.tenant_id
        from datai_config_snapshot dcs
    </sql>

    <select id="selectDataiConfigSnapshotList" parameterType="DataiConfigSnapshot" resultMap="DataiConfigSnapshotResult">
        <include refid="selectDataiConfigSnapshotVo"/>
        <where>
            <if test="versionId != null "> and dcs.version_id = #{versionId}</if>
            <if test="snapshotTime != null "> and dcs.snapshot_time = #{snapshotTime}</if>
            <if test="snapshotContent != null  and snapshotContent != ''"> and dcs.snapshot_content = #{snapshotContent}</if>
            <if test="configCount != null "> and dcs.config_count = #{configCount}</if>
            <if test="tenantId != null  and tenantId != ''"> and dcs.tenant_id = #{tenantId}</if>
        </where>
    </select>
    
    <select id="selectDataiConfigSnapshotBySnapshotId" parameterType="String" resultMap="DataiConfigSnapshotResult">
        <include refid="selectDataiConfigSnapshotVo"/>
        where dcs.snapshot_id = #{snapshotId}
    </select>
        
    <insert id="insertDataiConfigSnapshot" parameterType="DataiConfigSnapshot">
        insert into datai_config_snapshot
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="snapshotId != null">snapshot_id,</if>
            <if test="versionId != null">version_id,</if>
            <if test="snapshotTime != null">snapshot_time,</if>
            <if test="snapshotContent != null and snapshotContent != ''">snapshot_content,</if>
            <if test="configCount != null">config_count,</if>
            <if test="createBy != null and createBy != ''">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="remark != null">remark,</if>
            <if test="tenantId != null">tenant_id,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="snapshotId != null">#{snapshotId},</if>
            <if test="versionId != null">#{versionId},</if>
            <if test="snapshotTime != null">#{snapshotTime},</if>
            <if test="snapshotContent != null and snapshotContent != ''">#{snapshotContent},</if>
            <if test="configCount != null">#{configCount},</if>
            <if test="createBy != null and createBy != ''">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="remark != null">#{remark},</if>
            <if test="tenantId != null">#{tenantId},</if>
         </trim>
    </insert>

    <update id="updateDataiConfigSnapshot" parameterType="DataiConfigSnapshot">
        update datai_config_snapshot
        <trim prefix="SET" suffixOverrides=",">
            <if test="versionId != null">version_id = #{versionId},</if>
            <if test="snapshotTime != null">snapshot_time = #{snapshotTime},</if>
            <if test="snapshotContent != null and snapshotContent != ''">snapshot_content = #{snapshotContent},</if>
            <if test="configCount != null">config_count = #{configCount},</if>
            <if test="createBy != null and createBy != ''">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="tenantId != null">tenant_id = #{tenantId},</if>
        </trim>
        where datai_config_snapshot.snapshot_id = #{snapshotId}
    </update>

    <delete id="deleteDataiConfigSnapshotBySnapshotId" parameterType="String">
        delete from datai_config_snapshot where snapshot_id = #{snapshotId}
    </delete>

    <delete id="deleteDataiConfigSnapshotBySnapshotIds" parameterType="String">
        delete from datai_config_snapshot where snapshot_id in 
        <foreach item="snapshotId" collection="array" open="(" separator="," close=")">
            #{snapshotId}
        </foreach>
    </delete>
</mapper>